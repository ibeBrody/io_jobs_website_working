{
	"configurations": [
		{
			"name": "Python Debugger: Flask",
			"type": "debugpy",
			"request": "launch",
			"module": "flask",
			"env": { "FLASK_APP": "app.py", "FLASK_DEBUG": "1" },
			"args": ["run", "--no-debugger"],
			"jinja": true,
			"autoStartBrowser": false,
         "justMyCode": false
		}
	]
}
